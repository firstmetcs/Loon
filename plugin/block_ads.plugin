URL-REGEX, "^https?:\/\/www1\.elecfans\.com\/www\/delivery\/", REJECT

# >>>>>>>>>>>>>>> ✅ H ✅ <<<<<<<<<<<<<<

# 盒马
# https://github.com/sooyaaabo/Loon/blob/main/Plugin/MyBlockAds.plugin 822f1a3
AND, ((URL-REGEX, "^http:\/\/.+\/amdc\/mobileDispatch"), (USER-AGENT, "Hema4iPhone*")), REJECT

